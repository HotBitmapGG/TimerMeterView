<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--计时器-->
    <declare-styleable name="TimerMeterView">
        <attr name="line_bg" format="color" />
        <attr name="normal_text_color" format="color" />
        <attr name="select_text_color" format="color" />
        <attr name="text_size" format="integer" />
        <attr name="cyclic" format="boolean" />
        <attr name="timer_type" format="enum">
            <enum name="hours_mins" value="1" />
            <enum name="min_second" value="2" />
            <enum name="hour_min_second" value="3" />

        </attr>
    </declare-styleable>


</resources>